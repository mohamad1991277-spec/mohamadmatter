generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Citizen {
  id        String   @id @default(uuid())
  fullName  String
  dob       DateTime
  gender    String
  city      String
  area      String
  mobile    String
  need      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
